{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug mitmdump script",
        "type": "debugpy",
        "request": "launch",
        "program": "mitmdump",
        "args": [
          "-p", "8888",
          "--mode", "upstream:http://127.0.0.1:7890",
          "-s", "${workspaceFolder}/proxy.py"
        ],
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}"
      }
    ]
  }
  